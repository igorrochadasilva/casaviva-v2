    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "laravel-mix": "^6.0.34",
